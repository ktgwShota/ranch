{
	"name": "my-next-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "concurrently \"npm run dev:worker\" \"npm run dev:next\"",
		"dev:worker": "wrangler dev --port 8788",
		"dev:next": "next dev",
		"build": "next build",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"deploy:stg": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env stg",
		"deploy:prod": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env prod",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
		"db:setup": "wrangler d1 execute myapp-local --local --file=./src/worker/sqls/schema.sql",
		"db:setup:stg": "wrangler d1 execute myapp-stg --file=./src/worker/sqls/schema.sql -e stg",
		"db:setup:prod": "wrangler d1 execute myapp-prod --file=./src/worker/sqls/schema.sql -e prod",
		"db:query": "wrangler d1 execute myapp-local --local --command",
		"db:reset": "rm -rf .wrangler/state/v3/d1/ && npm run db:setup"
	},
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@mui/icons-material": "^7.3.2",
		"@mui/material": "^7.3.2",
		"framer-motion": "^12.23.12",
		"next": "15.4.6",
		"react": "19.1.0",
		"react-dom": "19.1.0"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20251004.0",
		"@opennextjs/cloudflare": "^1.9.1",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20.19.14",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"concurrently": "^9.2.1",
		"tailwindcss": "^4",
		"typescript": "^5",
		"wrangler": "^4.36.0"
	}
}
